import java.util.Scanner;

public class Baek11021{
    public static void main(String[] args){
        int n, num1, num2, total;
        Scanner in = new Scanner(System.in);

        n=in.nextInt();
        for(int k=1;k<=n;k++){
            num1=in.nextInt();
            num2=in.nextInt();
            total = num1+num2;
            System.out.println("Case #"+ k +": "+ total);
        }
    }
}
