import java.util.Scanner;

public class Baek10952{
    public static void main(String[] args){
        int num1, num2;
        Scanner in = new Scanner(System.in);

        do{
            num1=in.nextInt();
            num2=in.nextInt();
            if(num1+num2 != 0){
                System.out.println(num1+num2);
            }
        }
        while(num1 != 0);
            in.close();
    }
}
