import java.util.Scanner;

public class Baek2480{
    public static void main(String[] args){
        int a,b,c, max, reward;
        Scanner in = new Scanner(System.in);

        a=in.nextInt();
        b=in.nextInt();
        c=in.nextInt();

        if(a>b){
            max=a;
            if(max>c);
            else{
                max=c;
            }
        }
        else{
            max=b;
            if(max>c);
            else{
                max=c;
            }
        }

        if(a==b&&a!=c&&b!=c){
            reward=1000+a*100;
        }
        else if(a!=b&&a==c&&b!=c){
            reward=1000+a*100;           
        }
        else if(a!=b&&a!=c&&b==c){
            reward=1000+b*100;            
        }
        else if(a==b&&b==c){
            reward = 10000+a*1000;
        }
        else{
            reward=max*100;
        }
        System.out.println(reward);
    }
}
