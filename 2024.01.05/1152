import java.util.Scanner;

public class Baek1152{
    public static void main(String[] args){
        Scanner in = new Scanner(System.in);
        String a = in.next();
        int total=1;
        for(int i=0;i<a.length();i++){
            if(a.charAt(i)==' '){
                total++;
            }
        }
        System.out.println(total);
    }
}
