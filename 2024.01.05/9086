package test;
import java.util.Scanner;
public class test{
	public static void main (String[] args) {
		Scanner sc =new Scanner(System.in);	
		int T=sc.nextInt();
		for(int i=0;i<T;i++) {
			String a=sc.next();
			System.out.print(a.charAt(0));
			System.out.println(a.charAt(a.length()-1));
		}	
		sc.close();	
	}
}
