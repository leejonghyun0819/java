package test;
import java.util.Scanner;
public class test{
	public static void main (String[] args) {
		Scanner sc =new Scanner(System.in);	
		int[] number= new int[26]; 
		for(int i=0;i<26;i++) {
			number[i]=-1;
		}
		String ch=sc.next();
		for(int i=0;i<ch.length();i++) {
			char a=ch.charAt(i);
			if(number[a-'a']==-1) {
				number[a-'a']=i;
			}				
		}
		for(int i=0;i<26;i++) {
			System.out.print(number[i]+" ");
		}
		
		sc.close();	
	}
}
