import java.util.Scanner;

public class Baek11720{
    public static void main(String[] args){
        int sum=0,n;
        Scanner in = new Scanner(System.in);
        n = in.nextInt();
        String a=in.next();
        for(int i=0;i<a.length();i++){
            sum=a.charAt(i)-'0'+sum;
        } 
        System.out.println(sum);
    }
}
