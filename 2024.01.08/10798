package test;
import java.util.Scanner;
public class test{
	public static void main (String[] args) {
		Scanner sc =new Scanner(System.in);	
		char number[][]=new char[5][15];
		for(int i=0;i<5;i++) {
			String ch=sc.next();
			for(int j=0;j<ch.length();j++) {
				number[i][j]=ch.charAt(j);
			}	
		}
		for(int i=0;i<15;i++) {
			for(int j=0;j<5;j++) {
				if(number[j][i]!='\0') {
					System.out.print(number[j][i]);
				}
			}
		}
		sc.close();	
	}
}
