#pragma warning(disable:4996)
#include<stdio.h>
#include<string.h>
#include<stdlib.h>
int main() {
	int N, M,number[100],sum=0,result=0;
	scanf("%d %d", &N,&M);
	for (int i = 0; i < N; i++) {
		scanf("%d", &number[i]);
	}
	for (int i = 0; i < N-2; i++) {
		for (int j = i + 1; j < N - 1; j++) {
			for (int k = j + 1; k < N; k++) {
				sum = number[i] + number[j] + number[k];
				if (sum <= M && (M-sum)<(M-result)) {
					result = sum;
				}
			}
		}
		
	}
	printf("%d", result);
	return 0;
}


