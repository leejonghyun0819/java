#pragma warning(disable:4996)
#include<stdio.h>
#include<string.h>
#include<stdlib.h>
int main() {
	int N, sum=0;
	scanf("%d", &N);
	while (N>0) {
		if (N % 5 == 0) {
			N -=5;
			sum ++;

		}
		else {
			N -= 3;
			sum++;
		}
	}
	if (N==0&&sum!=0) {
		printf("%d", sum);
	}
	else {
		
		printf("-1"); 
	}
	return 0;
}


