#pragma warning(disable:4996)
#include<stdio.h>
#include<string.h>
#include<stdlib.h>
int main() {
	int a, b, c, d, e, f, x, y;
	scanf("%d %d %d %d %d %d", &a, &b, &c, &d, &e, &f);
	for (int x = -999; x <= 999;x++) {
		for (int y = -999; y <= 999; y++) {
			if (a*x + b * y == c && d*x + e * y == f) {
				printf("%d %d", x, y);
				break;
			}
		}
	}
	return 0;
}


