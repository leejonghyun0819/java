package test;
import java.util.Scanner;
public class test{
	public static void main (String[] args) {
		Scanner sc =new Scanner(System.in);
		int N=sc.nextInt();
		int sum=1,a=2;
		if(N==1) {
			System.out.print(1);
		}
		else {
			while(a<=N) {
				a+=sum*6;
				sum++;}
			System.out.print(sum);
		}
	
		
		
		sc.close();	
	}
}
