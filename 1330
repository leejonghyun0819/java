import java.util.Scanner;

public class Baek1330
{
    public static void main(String[] args){
        int A,B;
        Scanner in = new Scanner(System.in);

        A = in.nextInt();
        B = in.nextInt();

        if(A<B){
            System.out.println("<");
        }
        if(A==B){
            System.out.println("==");
        }
        if(A>B){
            System.out.println(">");
        }
    }
}
