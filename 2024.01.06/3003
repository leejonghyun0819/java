package test;
import java.util.Scanner;
public class test{
	public static void main (String[] args) {
		Scanner sc =new Scanner(System.in);	
		int[] number=new int[6];
		
		for(int i=0;i<6;i++) {
			number[i]=sc.nextInt();
		}	
		System.out.print(1-number[0]+" ");
		System.out.print(1-number[1]+" ");
		System.out.print(2-number[2]+" ");
		System.out.print(2-number[3]+" ");
		System.out.print(2-number[4]+" ");
		System.out.print(8-number[5]);
		
		
		
		sc.close();	
	}
}
