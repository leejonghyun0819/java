package test;
import java.util.Scanner;
public class test{
	public static void main (String[] args) {
		Scanner sc =new Scanner(System.in);	
		int A=sc.nextInt();
		int B=sc.nextInt();
		int V=sc.nextInt();
		int sum=0;
		sum=(V-A)/(A-B)+1;
		if((V-A)%(A-B)!=0) {
			sum++;
		}
		System.out.println(sum);
		
		sc.close();	
	}
}
