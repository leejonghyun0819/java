package test;
import java.util.Scanner;
public class test{
	public static void main (String[] args) {
		Scanner sc =new Scanner(System.in);
		int N=sc.nextInt();
		int K=sc.nextInt();
		int cnt=0,min3=0;
		for(int i=1;i<=N;i++) {
			if(N%i==0) {
				cnt++;
				}
				if(cnt==K) {
					min3=i;
					break;
				}
			}
		
		System.out.print(min3);
		sc.close();	
	}
}
