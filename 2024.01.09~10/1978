package test;
import java.util.Scanner;
public class test{
	public static void main (String[] args) {
		Scanner sc =new Scanner(System.in);
		int N=sc.nextInt();
		int cnt=0,sum=0;
		for(int j=0;j<N;j++) {
			int n=sc.nextInt();
			if(n!=1) {
		for(int i=2;i<n;i++) {
			if(n%i==0) {
				cnt++;
				}
			}
		if(cnt==0) {
			sum++;
			cnt=0;
		}
		}
		}
		System.out.print(sum);
		sc.close();	
	}
}
