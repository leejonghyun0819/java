package test;
import java.util.Scanner;
public class test {
	public static void main (String[] args) {
		Scanner sc =new Scanner(System.in);				
		int N=sc.nextInt();
		int[] number=new int[1000000];
		for(int i=0;i<N;i++) {	
			int a=sc.nextInt();
			number[i]=a;
			}
		int max=0;
		int min=number[0];
		for(int i=0;i<N;i++) {
			if(number[i]>=max) {
				max=number[i];
			}
			if(number[i]<=min) {
				min=number[i];
			}
		}
		System.out.print(min+" "+max);
		
		sc.close();	
	}
}
