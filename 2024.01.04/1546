package test;
import java.util.Scanner;
public class test {
	public static void main (String[] args) {
		Scanner sc =new Scanner(System.in);						
		float[] number=new float[1000];
		int N=sc.nextInt();
		float M=number[0];
		float sum=0;
		for(int i=0;i<N;i++) {
			number[i]=sc.nextInt();
			if(number[i]>=M) {
				M=number[i];
			}			
		}
		for(int i=0;i<N;i++) {
			number[i]=number[i]/M*100;
			sum=sum+number[i];
		}
		System.out.println(sum/N);
		
		
		sc.close();	
	}
}
