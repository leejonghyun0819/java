package test;
import java.util.Scanner;
public class test {
	public static void main (String[] args) {
		Scanner sc =new Scanner(System.in);				
		int N=sc.nextInt();
		int result=0;
		int[] number=new int[100];
		for(int i=0;i<N;i++) {	
			int a=sc.nextInt();
			number[i]=a;
			}
		int v=sc.nextInt();
		for(int i=0;i<N;i++) {	
			if(number[i]==v) {
				result++;
			}
			}
		System.out.println(result);
		sc.close();	
	}
}
