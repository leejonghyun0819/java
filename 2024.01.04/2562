import java.util.Scanner;

public class Baek2562{
    public static void main(String[] args){
        int num=0, max=-1;
        int arr[] = new int[9];
        Scanner in = new Scanner(System.in);

        for(int i=0;i<arr.length;i++){
            arr[i]=in.nextInt();
            if(arr[i]>max){
                max=arr[i];
                num=i+1;
            }
        }
        System.out.printf("%d\n%d\n", max, num);
    }
}
