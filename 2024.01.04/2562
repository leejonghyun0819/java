package test;
import java.util.Scanner;
public class test {
	public static void main (String[] args) {
		Scanner sc =new Scanner(System.in);				
		
		int[] number=new int[9];
		for(int i=0;i<9;i++) {	
			int a=sc.nextInt();
			number[i]=a;
			}
		int max=number[0];
		int a=0;
		
		
		for(int i=0;i<9;i++) {
			if(number[i]>=max) {
				max=number[i];
				a=i;
			}
			
			
		}
		System.out.println(max);
		System.out.print(a+1);
		
		sc.close();	
	}
}
