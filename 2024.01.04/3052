package test;
import java.util.Scanner;
public class test {
	public static void main (String[] args) {
		Scanner sc =new Scanner(System.in);						
		int[] number=new int[10];
		int a=0;//다른수의 개수
		for(int i=0;i<10;i++) {
		number[i]=sc.nextInt()%42;
		}
		for(int i=0;i<10;i++) {
			int b=0;
			for(int j=i+1;j<10;j++) {
				if(number[i]==number[j]) {
					b++;
				}			
			}
			if(b==0) {
				a++;
			}
		}
		System.out.println(a);
		
		
		sc.close();	
	}
}
